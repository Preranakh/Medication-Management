<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    tools:context="com.testreminder.reminder.MedList">

    <Button
        android:id="@+id/AddMed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="+"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.857"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Medications"
        android:layout_width="174dp"
        android:layout_height="52dp"
        android:layout_marginBottom="23dp"
        android:text="Medications"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/Med1"
        app:layout_constraintEnd_toStartOf="@+id/AddMed"
        app:layout_constraintHorizontal_bias="0.553"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.173" />

    <Button
        android:id="@+id/Med1"
        android:layout_width="168dp"
        android:layout_height="72dp"
        android:layout_marginTop="75dp"
        android:layout_marginBottom="10dp"
        android:onClick="openMedBlood"
        android:text="BloodPressure"
        app:layout_constraintBottom_toTopOf="@+id/Med2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/AddMed"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/Med2"
        android:layout_width="165dp"
        android:layout_height="64dp"
        android:layout_marginBottom="372dp"
        android:onClick="openMedAnxiety"
        android:text="Anti-Anxiety"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>